{
  "name": "ForgeHub",
  "version": "0.0.1",
  "description": "Prototype of Forge's IOS/Android App",
  "author": "Sanath Mullapudi",
  "engines": {
    "node": "6.6.0"
  },
  "private": true,
  "scripts": {
    "build-ios": "react-native bundle --entry-file='index.ios.js' --bundle-output='./app.bundledJS/main.jsbundle' --reset-cache --dev=false --platform='ios' --assets-dest='./ios'",
    "packager": "node node_modules/react-native/local-cli/cli.js start",
    "scrapper": "babel-node ./graphql/data/database/tempLoader.js",
    "updatedServer": "npm run update-schema && npm run start",
    "start": "babel-node ./graphql/server.js",
    "update-schema": "babel-node ./graphql/scripts/updateSchema.js"
  },
  "dependencies": {
    "babel-plugin-transform-function-bind": "6.8.0",
    "babel-preset-react-native": "1.9.0",
    "babel-relay-plugin": "0.9.3",
    "express": "4.14.0",
    "express-graphql": "0.5.4",
    "graphql": "0.7.0",
    "graphql-relay": "0.4.3",
    "jwt-simple": "0.5.0",
    "lodash": "4.15.0",
    "mongoose": "4.6.0",
    "react": "15.3.1",
    "react-native": "0.33.0",
    "react-native-orientation": "1.17.0",
    "react-native-router-flux": "3.35.0",
    "react-native-scrollable-tab-view": "0.6.0",
    "react-native-vector-icons": "2.1.0",
    "react-native-video": "0.9.0",
    "react-relay": "0.9.3",
    "react-relay-network-layer": "1.3.4"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "selenium-webdriver": "3.0.0-beta-2"
  }
}
