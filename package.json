{
  "name": "ForgeHub",
  "version": "0.0.1",
  "description": "Prototype of Forge's IOS/Android App",
  "author": "Sanath Mullapudi",
  "private": true,
  "scripts": {
    "build-ios": "react-native bundle --entry-file='index.ios.js' --bundle-output='./app.bundledJS/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "packager": "node node_modules/react-native/local-cli/cli.js start",
    "scrapper": "babel-node ./graphql/data/database/load.js",
    "server": "npm run update-schema && babel-node ./graphql/server.js",
    "update-schema": "babel-node ./graphql/scripts/updateSchema.js"
  },
  "dependencies": {
    "babel-preset-react-native": "1.9.0",
    "babel-relay-plugin": "0.9.3",
    "express": "4.14.0",
    "express-graphql": "0.5.4",
    "graphql": "0.7.0",
    "graphql-relay": "0.4.3",
    "mongoose": "4.6.0",
    "react": "15.3.1",
    "react-native": "0.33.0",
    "react-native-scrollable-tab-view": "0.6.0",
    "react-native-vector-icons": "2.1.0",
    "react-relay": "0.9.3"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "selenium-webdriver": "3.0.0-beta-2"
  }
}
